    <link href="~/Content/Plugin/Ueditor/themes/default/css/ueditor.css" rel="stylesheet" />
    <link href="~/Content/Plugin/Ueditor/third-party/codemirror/codemirror.css" rel="stylesheet" />
    <link href="~/Content/Css/Person/person.css" rel="stylesheet" />
}
@section MySection1{
    <script src="~/Content/Plugin/bootstrap-fileinput/js/fileinput.js"></script>
    <script src="~/Content/Plugin/Ueditor/ueditor.config.js"></script>
    <script src="~/Content/Plugin/Ueditor/ueditor.all.js"></script>
