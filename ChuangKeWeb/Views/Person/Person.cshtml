                        <span class="label">文章配图</span>
                        <div class=" kv-main">
                            <br>
                            <form enctype="multipart/form-data">
                                <div class="form-group">
                                    <input id="file-1" type="file" data-overwrite-initial="false" data-min-file-count="1">
                                </div>

                            </form>
                        </div>
                    </div>
                </div>
                <div class="third-text">
                    <span class="label">文章内容</span>
                    <script id="container" name="content" type="text/plain">
                        编辑作品内容
                    </script>
                </div>
            </div>
            <div class="match-2">
                <div class="third-title">
                    <div class="input-group">
                        <div class="input-group-addon">文章配图</div>
                        <input type="File" class="form-control" placeholder="请认真提交">
                    </div>
