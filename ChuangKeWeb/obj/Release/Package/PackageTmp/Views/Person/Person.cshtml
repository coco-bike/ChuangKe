<div class="container">
    <div class="jumbotron">
        <div class="container">
            <div class="col-md-5 head">
                <div class="head-ico">
                    <img src="../../Files/ico/man.png" alt="男女">
                </div>
                <h2>李四</h2>
                <p>C五年级2班</p>
            </div>
            <div class="col-md-7 ck-head-right">
                <div class="ck-ad">
                </div>
                <button type="button" class="btn btn-large btn-block btn-success">编辑资料</button>
            </div>
        </div>
    </div>
    <div class="ck-studentmessage">
        <div class="ck-samllnav">
            <ul>
                <li class="col-md-4 "><a href="javascript:void(0);" onclick="changefirstli()">个人资料</a></li>
                <li class="col-md-4"><a href="javascript:void(0);" onclick="changesecondli()">报名资料</a></li>
                <li class="col-md-4"><a href="javascript:void(0);" onclick="changethirdli()">参赛作品</a></li>
                <div class="clearfloat"></div>
            </ul>
        </div>
        <div class="first-li li-style">
            <form action="" method="POST" class="form-inline" role="form">
                <div class="form-group col-md-6">
                    <label class="ck-lable" for="">姓名</label>
                    <input type="name" class="form-control" id="name" placeholder="姓名" readonly="readonly">
                </div>
                <div class="form-group col-md-6">
                    <label class="ck-lable" for="">性别</label>
                    <select id="city" class="select" disabled="disabled">
                        <option value="1">男</option>
                        <option value="0">女</option>
                    </select>
                </div>
                <div class="form-group col-md-6">
                    <label class="ck-lable" for="">身份证</label>
                    <input type="id" class="form-control" id="Idcard" placeholder="身份证" readonly="readonly">
                </div>
                <div class="form-group col-md-6">
                    <label class="ck-lable" for="">出生日期</label>
                    <input type="date" class="form-control" id="birthdate" placeholder="出生日期" readonly="readonly">
                </div>
                <div class="form-group col-md-6">
                    <label class="ck-lable" for="">邮箱</label>
                    <input type="email" class="form-control" id="email1" placeholder="邮箱" readonly="readonly">
                </div>
                <div class="form-group col-md-6">
                    <label class="ck-lable" for="">手机号</label>
                    <input type="telnumber" class="form-control" id="number" placeholder="手机号" readonly="readonly">
                </div>
                <div class="form-group col-md-6">
                    <label class="ck-lable" for="">学段</label>
                    <input type="text" class="form-control" id="schoollevel" placeholder="学段" readonly="readonly">
                </div>
                <div class="form-group col-md-6">
                    <label class="ck-lable" for="">学号</label>
                    <input type="text" class="form-control" id="schoolnumber" placeholder="学号" readonly="readonly">
                </div>
                <div class="form-group col-md-6">
                    <label class="ck-lable" for="">入学年月</label>
                    <input type="date" class="form-control" id="schooldate" placeholder="入学年份" readonly="readonly">
                </div>
                <div class="clearfloat">

                </div>
            </form>
            <button type="submit" class="btn btn-primary">保存</button>
        </div>
        <div class="second-li li-style">
            <form action="" method="POST" class="form-inline" role="form">
                <div class="form-group col-md-12">
                    <label class="ck-lable" for="">地区</label>
                    <input type="name" class="form-control" id="place" placeholder="地区" readonly="readonly">
                </div>
                <div class="form-group col-md-12">
                    <label class="ck-lable" for="">学段</label>
                    <input type="sex" class="form-control" id="matchschoollevel" placeholder="学段" readonly="readonly">
                </div>
                <div class="form-group col-md-12">
                    <label class="ck-lable" for="">学校</label>
                    <input type="id" class="form-control" id="school" placeholder="学校" readonly="readonly">
                </div>
                <div class="form-group col-md-12">
                    <label class="ck-lable" for="">比赛项目</label>
                    <input type="text" class="form-control" id="match" placeholder="比赛项目" readonly="readonly">
                </div>
                <div class="form-group col-md-12">
                    <label class="ck-lable" for="">学生姓名</label>
                    <input type="text" class="form-control" id="names" placeholder="学生姓名,多个请用,逗号隔开" readonly="readonly">
                </div>
                <div class="form-group col-md-12">
                    <label class="ck-lable" for="">指导老师</label>
                    <input type="number" class="form-control" id="teachers" placeholder="指导老师,多个请用,逗号隔开" readonly="readonly">
                </div>
                <div class="form-group col-md-12">
                    <label class="ck-lable" for="">教师电话</label>
                    <input type="text" class="form-control" id="teachernumber" placeholder="教师电话,多个请用,逗号隔开" readonly="readonly">
                </div>
                <div class="clearfloat">
                </div>
            </form>
            <button type="submit" class="btn btn-primary">保存</button>
        </div>
